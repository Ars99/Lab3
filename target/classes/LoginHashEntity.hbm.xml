<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.LoginHashEntity" table="login_hash" schema="public" catalog="login_password">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="login">
            <column name="login" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="hash">
            <column name="hash" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>